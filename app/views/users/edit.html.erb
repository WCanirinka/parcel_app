<% provide(:title, "Edit Profile") %>
<h1>Profile settings</h1>
<h1>Welcome, <%= @user.name %>!</h1>
<br><br><br>
<div class = "container">
<div class = "jumbotron col-md-4 col-md-offset-4" >
  
<%= form_for (@user) do |f| %>
  <%= f.label :Phone %>
  <%= f.text_field :phone, class: "form-control"%>
  <br>
  <%= f.submit "Update phone number", class: "btn" %>
<% end %>
</div>
</div>
<br>